<?xml version="1.0" encoding="UTF-8"?>
<extension id="db_sync" status="released" xmlns="http://symphony-cms.com/schemas/extension/1.0">
	<name>Database Synchroniser</name>
	<description>Log database changes to easily synchronise structure (sections, fields, pages) development and production databases, without affecting content.</description>
	<repo type="github">https://github.com/symphonists/db_sync</repo>
	<url type="discuss">http://symphony-cms.com/discuss/thread/26748/</url>
	<types>
		<type>Deployment</type>
	</types>
	<authors>
		<author>
        		<name github="symphonists" symphony="community">Symphony Community</name>
    		</author>
		<author>
			<name github="nickdunn" symphony="nickdunn">Nick Dunn</name>
			<website>http://nick-dunn.co.uk</website>
			<name github="firegoby" symphony="firegoby">Chris Batchelor</name>
			<email>chris@firegoby.com</email>
			<website>http://firegoby.com</website>
		</author>
	</authors>
	<releases>
		<release version="1.1.0" date="2016-03-08" min="2.4.x" />
		<release version="1.0.0" date="2012-03-15" min="2.3.x" max="2.4.x" />
		<release version="0.9.1" date="2011-02-10" min="2.1.0" max="2.2.x" />
	</releases>
</extension>
